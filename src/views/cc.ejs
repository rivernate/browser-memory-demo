<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>CC Memory Example</title>
    <style>
        body {
            display: flex;
            flex-wrap: wrap;
        }

        .box {
            width: 100px;
            height: 100px;
            background-color: red;
            position: relative;
            margin: 10px;
            animation: move 5s infinite;
        }

        @keyframes move {
            0% { top: 0; left: 0; }
            50% { top: 200px; left: 200px; }
            100% { top: 0; left: 0; }
        }
    </style>
</head>
<body>
    <h1>CC Memory Example</h1>
    <div class="boxes">
        <% for (let i = 0; i < 100; i++) { %>
            <div class="box"></div>
        <% } %>
    </div>
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const boxesContainer = document.querySelector('.boxes');

            for (let i = 0; i < 100; i++) {
                const box = document.createElement('div');
                box.className = 'box';
                boxesContainer.appendChild(box);
            }
        });
    </script>
</body>
</html>

